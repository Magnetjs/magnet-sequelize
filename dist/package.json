{
  "name": "magnet-koa",
  "version": "1.0.1",
  "description": "Readme",
  "main": "dist/index.js",
  "scripts": {
    "start": "babel src --out-dir dist",
    "watch": "nodemon --ignore dist --exec \"babel src\" --out-dir dist",
    "watch-test": "nodemon --delay 0.5 --ignore src --exec babel-node tests/index.js | tap-spec",
    "watch-all": "concurrently \"npm run watch\" \"npm run watch-test\"",

    "build": "npm run clean-lib && npm run build:index && npm run copy-package",
    "build:index": "babel src --out-dir dist",
    "clean-lib": "node_modules/.bin/rimraf ./dist",
    "copy-package": "cp -rf package.json LICENSE README.md dist",
    "dist": "npm run build && cd dist && npm publish"
  },
  "keywords": [
    "magnet",
    "magnetjs",
    "koa"
  ],
  "author": "Kieve Chua <kievechua@gmail.com> (https://github.com/kievechua)",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.0.0",
    "magnet-core": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.6",
    "babel-plugin-transform-async-to-generator": "^6.7.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.0.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.1.0",
    "nodemon": "^1.9.1",
    "standard": "^8.6.0",
    "tap-spec": "^4.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Magnetjs/magnet-koa.git"
  },
  "bugs": {
    "url": "https://github.com/Magnetjs/magnet-koa/issues"
  },
  "homepage": "https://github.com/Magnetjs/magnet-koa#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  }
}

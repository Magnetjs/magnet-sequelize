{
  "name": "magnet-sequelize",
  "version": "0.3.0",
  "description": "Readme",
  "keywords": [
    "magnet",
    "magnetjs",
    "sequelize"
  ],
  "homepage": "https://github.com/Magnetjs/magnet-sequelize#readme",
  "bugs": {
    "url": "https://github.com/Magnetjs/magnet-sequelize/issues"
  },
  "license": "MIT",
  "author": "Kieve Chua <kievechua@gmail.com> (https://github.com/kievechua)",
  "main": "dist/index.js",
  "repository": "magnetjs/magnet-sequelize",
  "scripts": {
    "build": "npm run clean-lib && npm run build:index && npm run copy-package",
    "build:index": "babel src --out-dir dist",
    "clean-lib": "node_modules/.bin/rimraf ./dist",
    "copy-package": "cp -rf package.json LICENSE README.md dist",
    "dist": "npm run build && cd dist && npm publish",
    "start": "babel src --out-dir dist",
    "watch": "nodemon --ignore dist --exec \"babel src\" --out-dir dist",
    "watch-all": "concurrently \"npm run watch\" \"npm run watch-test\"",
    "watch-test": "nodemon --delay 0.5 --ignore src --exec babel-node tests/index.js | tap-spec"
  },
  "babel": {
    "ignore": [
      "docs",
      "node_modules",
      "dist",
      "test"
    ],
    "plugins": [
      "fast-async"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "magnet-core": "^3.3.0",
    "pg": "^6.1.2",
    "sequelize": "^3.30.2"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "concurrently": "^3.3.0",
    "fast-async": "^6.2.0",
    "nodemon": "^1.9.1",
    "rimraf": "^2.5.4",
    "standard": "^8.6.0",
    "tap-spec": "^4.1.1"
  },
  "engines": {
    "node": ">=6.9.5"
  }
}
